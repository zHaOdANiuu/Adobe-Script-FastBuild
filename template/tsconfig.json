{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "jsx": "preserve",
    "strict": true,
    "noEmit": true,
    "noLib": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "useDefineForClassFields": false
  },
  "include": [
    "env.d.ts",
    "index.tsx",
    "src"
  ]
}